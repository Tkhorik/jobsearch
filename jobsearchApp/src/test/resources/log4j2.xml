<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="info">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} %-5level %class{36} %L %M - %msg%xEx%n"/>
        </Console>
    </Appenders>
    <root>
        <level value="OFF"/>
        <appender-ref ref="Console"/>
    </root>
</Configuration>